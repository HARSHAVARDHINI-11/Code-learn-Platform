{
  "name": "codelearn-platform",
  "version": "2.0.0",
  "description": "Professional Social Learning Platform for Coding - Spring Boot + React",
  "scripts": {
    "start:backend": "cd backend-spring && mvn spring-boot:run",
    "start:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build:backend": "cd backend-spring && mvn clean package -DskipTests",
    "install:frontend": "cd frontend && npm install",
    "legacy:start": "node backend/server.js",
    "legacy:server": "nodemon backend/server.js",
    "legacy:dev": "concurrently \"npm run legacy:server\" \"npm run start:frontend\""
  },
  "keywords": ["spring-boot", "react", "coding", "learning", "social", "java"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
